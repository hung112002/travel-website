            "#f30909": "/all-destinations?search=川本町",
            "#efa300": "/all-destinations?search=出雲市",
            "#07c5ff": "/all-destinations?search=浜田市",
            "#0065fd": "/all-destinations?search=江津市",
            "#0657f9": "/all-destinations?search=益田市",
            "#09c6ff": "/all-destinations?search=津和野町",
            "#4208f2": "/all-destinations?search=吉賀町",
            "#ff9808": "/all-destinations?search=奥出雲町"
        };

        const regionNames = {
            "#f20467": "松江エリア (Vùng Matsue)",
            "#ece3b4": "石見エリア (Vùng Iwami)",
            "#00ff08": "安来エリア (Vùng Yasugi)",
            "#ff9808": "奥出雲エリア (Vùng奥出雲)",
            "#ec0a0a": "飯南エリア (Vùng Iinan)",
            "#f30909": "川本エリア (Vùng Kawamoto)",
            "#00a1fe": "邑南エリア (Vùng Ōnan)",
            "#f20c59": "美郷エリア (Vùng Misato)",
            "#b6f2dc": "隠岐エリア (Vùng đảo Oki)",
            "#efa300": "出雲エリア (Vùng Izumo)",
            "#ff0d00": "大田エリア (Vùng Oda)",
            "#0065fd": "江津エリア (Vùng Gotsu)",
            "#07c5ff": "浜田エリア (Vùng Hamada)",
            "#0657f9": "益田エリア (Vùng Masuda)",
            "#09c6ff": "津和野エリア (Vùng Tsuwano)",
            "#4208f2": "吉賀町エリア (Vùng 吉賀町)"

        };

        
        const areaTitle = document.getElementById('current-area-title');
        const areaDesc = document.getElementById('current-area-desc');

     
        const mapElements = document.querySelectorAll('.map-svg-container use, .map-svg-container path');

        mapElements.forEach(el => {
          
            const style = el.getAttribute('style');
            
            if (style) {
               
                for (const [color, link] of Object.entries(regionLinks)) {
                    if (style.includes(color)) {
                        
                        
                        el.style.cursor = "pointer";
                        el.style.transition = "opacity 0.3s";

                       
                        el.onclick = function() {
                            window.location.href = link;
                        };

                      
                        el.onmouseover = function() {
                            this.style.opacity = "0.7"; 
                            areaTitle.innerText = regionNames[color];
                            areaDesc.innerText = "クリックして、このエリアの観光地を表示します。";
                        };

                        el.onmouseout = function() {
                            this.style.opacity = "1";
                            areaTitle.innerText = "選択してください";
                            areaDesc.innerText = "地図上のエリアをクリックして、詳細情報を確認してください。";
                        };
                    }
                }
            }
        });
    });
    

</script>
